script/source = "extends Control

var selected = \"\"
var mouseEntered : bool = false
var spawnBuffered : bool = false
var block = preload(\"res://Scenes/Block.tscn\")

# yo hey you need to make the mouse entered work more 

func _physics_process(delta):
	print(mouseEntered)
	_findSelected()
	if Input.is_action_just_pressed(\"Click\") and mouseEntered == true:
		spawnBuffered = true
	elif Input.is_action_just_released(\"Click\"):
		spawnBuffered = false
	
	if spawnBuffered == true and get_global_mouse_position().y > 400 - 40:
		_spawn(selected)
		spawnBuffered = false


func _spawn(selection):
	var b = block.instantiate()
	
	match selection:
		\"serve\":
			b.text = \"Serve\"
			b.length = 2
		\"regular\":
			b.text = \"Regular\"
			b.length = 2
		\"slow\":
			b.text = \"Slow\"
			b.length = 2
		\"superfast\":
			b.text = \"Superfast\"
			b.length = 2
		\"fast\":
			b.text = \"Fast\"
			b.length = 6
		\"superbaked\":
			b.text = \"SuperfastB\"
		\"tinkStart\":
			b.text = \"TinkStart\"
		\"tinkEnd\":
			b.text = \"TinkEnd\"
			
	add_child(b)

func _findSelected():
	get_tree().call_group(\"Selection\",\"_findSelected\")




func _on_blocks_mouse_entered():
	mouseEntered = true


func _on_blocks_mouse_exited():
	mouseEntered = false


func _allButtonMouse():
	mouseEntered = true
"

