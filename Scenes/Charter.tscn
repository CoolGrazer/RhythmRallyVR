script/source = "extends Control

var selected = \"\"
var mouseEntered : bool = false
var spawnBuffered : bool = false
var block = preload(\"res://Scenes/Block.tscn\")
var strech = preload(\"res://Scenes/Stretchable.tscn\")
var paused : bool = true
var songPos : float = 0.0

func _ready():
	get_tree().paused = true

# charting

func _physics_process(delta):
	get_tree().paused = paused
	
	_findSelected()
	if Input.is_action_just_pressed(\"Click\") and mouseEntered == true:
		spawnBuffered = true
	elif Input.is_action_just_released(\"Click\"):
		spawnBuffered = false
	
	if spawnBuffered == true and get_global_mouse_position().y > 400 + 40:
		_spawn(selected)
		spawnBuffered = false


func _spawn(selection):
	var b = block.instantiate()
	
	match selection:
		\"serve\":
			b = strech.instantiate()
			b.text = \"Serve\"
			b.length = 2
			b.properties = [[0,\"serve\"]]
		\"regular\":
			b.text = \"Regular\"
			b.length = 4
			b.properties = [[0,\"reg\"]]
		\"slow\":
			b.text = \"Slow\"
			b.length = 8
			b.properties = [[0,\"slow\"]]
		\"superfast\":
			b.text = \"S.F\"
			b.length = 2
			b.properties = [[0,\"sFast\"]]
		\"fast\":
			b.text = \"Fast\"
			b.length = 6
			b.properties = [[0,\"tinkS\"],[1.5,\"tinkE\"],[2,\"fast\"]]
		\"superbaked\":
			b.text = \"SuperfastB\"
			b.length = 8
			b.properties = [[0,\"tinkS\"],[4,\"sFast\"],[8,\"reg\"]]
		\"tinkStart\":
			b.text = \"TinkStart\"
			b.length = 2
			b.properties = [[0,\"tinkS\"]]
		\"tinkEnd\":
			b.text = \"TinkEnd\"
			b.length = 2
			b.properties = [[0,\"tinkE\"]]
		\"victory\":
			b.text = \"Victory\"
			b.length = 2
			b.properties = [[0,\"victory\"]]
			
	add_child(b)
	#b.global_position = Vector2(500,500)
	

func _findSelected():
	get_tree().call_group(\"Selection\",\"_findSelected\")

func _generateChart():
	var beatPlayingFrom = $ColorRect.beat
	var sortedChart1 = []
	var sortedChart2 = []
	var sortedChart3 = []
	var sortedChart4 = []
	var unsortedNodes1 = []
	var unsortedNodes2 = []
	var unsortedNodes3 = []
	var unsortedNodes4 = []
	
	for x in get_tree().get_nodes_in_group(\"Block\"):
		if x.position.y == 464:
			unsortedNodes1.append(x)
		if x.position.y == 504:
			unsortedNodes2.append(x)
		if x.position.y == 544:
			unsortedNodes3.append(x)
		if x.position.y == 584:
			unsortedNodes4.append(x)
	
	
	for x in unsortedNodes1:
		sortedChart1.append_array(x._returnProperties())
	for x in unsortedNodes2:
		sortedChart2.append(x._returnProperties())
	for x in unsortedNodes3:
		sortedChart3.append(x._returnProperties())
	for x in unsortedNodes4:
		sortedChart4.append(x._returnProperties())
	
	
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.chart1 = sortedChart1
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.chart2 = sortedChart2
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.chart3 = sortedChart3
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.chart4 = sortedChart4
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.indx1 = 0
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.indx2 = 0
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.indx3 = 0
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.indx4 = 0
	
	#for x in sortedChart1:
	#	if x[0] < beatPlayingFrom:
	#		$Camera2D/SubViewportContainer/SubViewport/Node3D/Chart.indx1 += 1
	
	

func sort_ascending(a, b):
	if a[0] < b[0]:
		return true
	return false


func _on_blocks_mouse_entered():
	mouseEntered = true


func _on_blocks_mouse_exited():
	mouseEntered = false


func _allButtonMouse():
	mouseEntered = true


func _on_play_pressed():
	if paused == true:
		_play()


func _play():
	$Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer.play()
	$Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer.seek(songPos)
	GlobalValues.songInBeats = songPos / (60 / GlobalValues.bpm)
	paused = false
	$ColorRect.playing = true
	_generateChart()
	get_tree().paused = paused
	$Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer.emit_signal(\"beat\",0)


func _on_pause_pressed():
	if paused == false:
		_pause()

func _pause():
	songPos = $Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer.song_position
	$Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer.stop()
	paused = true
	$ColorRect.playing = false


func _on_stop_pressed():
	_stop()

func _stop():
	songPos = 0.0
	$Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer.stop()
	$Camera2D/SubViewportContainer/SubViewport/Node3D/AudioStreamPlayer._restart()
	$Camera2D/SubViewportContainer/SubViewport/Node3D/Path3D/PathFollow3D/CharacterBody3D.hide()
	paused = true
	$ColorRect.playing = false
	$Camera2D/SubViewportContainer/SubViewport/Node3D.restart()
"

